import static org.junit.Assert.*;
import java.io.FileNotFoundException;
import java.util.ArrayList;
import java.util.List;
import org.junit.Test;

public class test_IO {

	@Test
	public void IO() throws FileNotFoundException 
	{		
		//zawartość pliku testowego .csv
		/*
		Frequency;Front;Back;Difference
		123,3;124,4;125,5;126,6
		133,7;134,2;135,9;136,1
		143,5;144,6;145,3;146,9
		173,4;174,2;162,4;164,2
		192,2;172,4;195,4;191,8
		*/
		
		//wczytanie pliku testowego
		String path = "path\\file_X.csv";		
		List<Double[]> listaWczytana = IO.readCSV(path);
		
		//inicjalizacja listy oczekiwanej i dodanie wartości z pliku testowego
		List<Double[]> listaOczekiwana = new ArrayList<Double[]>();		
		Double[] d1 = {123.3, 124.4, 125.5, 126.6};
		Double[] d2 = {133.7, 134.2, 135.9, 136.1};
		Double[] d3 = {143.5, 144.6, 145.3, 146.9};
		Double[] d4 = {173.4, 174.2, 162.4, 164.2};
		Double[] d5 = {192.2, 172.4, 195.4, 191.8};		
		listaOczekiwana.add(d1);
		listaOczekiwana.add(d2);
		listaOczekiwana.add(d3);
		listaOczekiwana.add(d4);
		listaOczekiwana.add(d5);
		
		//porównanie zawartości obu list
		assertArrayEquals(listaWczytana.toArray(), listaOczekiwana.toArray());
	}
	
}
